<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- Apps -->

	<bean id="keaddonexample.app.example" factory-bean="kenyacore.appFactoryBean" factory-method="createInstance">
		<constructor-arg value="keaddonexample.example" />
		<constructor-arg value="SWOP" />
		<constructor-arg value="keaddonexample/example/home.page" />
		<constructor-arg value="keaddonexample:apps/swop-logo.png" />
		<constructor-arg value="700" />
	</bean>

	<!-- Program  -->
	<bean id="swop.program" class="org.openmrs.module.kenyacore.program.ProgramDescriptor">
		<property name="targetUuid" value="43b65fda-8c25-4478-a24c-bcc90e877f55" />
		<property name="eligibilityCalculation" value="org.openmrs.module.keaddonexample.calculation.EligibleForExampleProgramCalculation" />
		<property name="defaultEnrollmentForm" ref="keaddonexample.form.swopClientEnrollment"  />
		<property name="defaultCompletionForm" ref="keaddonexample.form.swopClientDiscontinuation" />
		<property name="patientForms">
			<set>
				<ref bean="keaddonexample.form.swopClientEnrollment" />
			</set>
		</property>
		<property name="fragments">
			<map>
				<entry key="enrollment-summary" value="keaddonexample:program/swop/swopEnrollmentSummary"/>
				<entry key="care-panel" value="keaddonexample:program/swop/swopCarePanel"/>
				<entry key="completion-summary" value="keaddonexample:program/swop/swopCompletionSummary"/>
			</map>
		</property>
		<property name="order" value="600" />


	</bean>
	<!-- Forms -->
	<!-- Forms config -->
	<bean id="keaddonexample.config.form" class="org.openmrs.module.kenyacore.form.FormConfiguration">
		<property name="commonVisitForms">
			<set>
				<ref bean="keaddonexample.form.example" />
				<ref bean="keaddonexample.form.socialworkadultscreening" />

			</set>
		</property>
	</bean>

	<!-- Forms addition -->
	<bean id="keaddonexample.form.example" class="org.openmrs.module.kenyacore.form.FormDescriptor">
		<property name="targetUuid" value="b694b1bc-2086-47dd-a4ad-ba48f9471e4b" />
		<property name="apps">
			<set>
				<ref bean="kenyaemr.app.clinician" />
				<ref bean="kenyaemr.app.chart" />
			</set>
		</property>
		<property name="icon" value="kenyaui:forms/generic.png" />
		<property name="htmlform" value="keaddonexample:example.html" />
	</bean>
	<!-- Social work Forms -->

	<bean id="keaddonexample.form.socialworkadultscreening" class="org.openmrs.module.kenyacore.form.FormDescriptor">
		<property name="targetUuid" value="0640058c-aa3f-47d0-867b-3025f2e5f80d" />
		<property name="apps">
			<set>
				<ref bean="kenyaemr.app.clinician" />
				<ref bean="kenyaemr.app.chart" />
			</set>
		</property>
		<property name="icon" value="kenyaui:forms/generic.png" />
		<property name="htmlform" value="keaddonexample:socialworkadultscreening.html" />
	</bean>
	<!-- swop enrollment form -->
	<bean id="keaddonexample.form.swopClientEnrollment" class="org.openmrs.module.kenyacore.form.FormDescriptor">
		<property name="targetUuid" value="4c620970-1364-414f-99d3-496dd120cb4b" />
		<property name="apps">
			<set>
				<ref bean="kenyaemr.app.registration" />
				<ref bean="kenyaemr.app.clinician" />
				<ref bean="kenyaemr.app.chart" />
				<ref bean="kenyaemr.app.hts" />
			</set>
		</property>
		<property name="icon" value="kenyaui:forms/generic.png" />
		<property name="htmlform" value="keaddonexample:swopClientEnrollment.html" />
		<property name="order" value="231018" />
	</bean>

	<!--SWOP Completion -->
	<bean id="keaddonexample.form.swopClientDiscontinuation" class="org.openmrs.module.kenyacore.form.FormDescriptor">
		<property name="targetUuid" value="cb62b9ab-d24a-4a52-aad2-84c2c7d79e5d" />
		<property name="apps">
			<set>
				<ref bean="kenyaemr.app.registration" />
				<ref bean="kenyaemr.app.intake" />
				<ref bean="kenyaemr.app.clinician" />
				<ref bean="kenyaemr.app.chart" />
			</set>
		</property>
		<property name="icon" value="kenyaui:forms/completion.png" />
		<property name="htmlform" value="keaddonexample:swopClientDiscontinuation.html" />
	</bean>

	<!-- Reports -->

	<bean id="kenyaemr.config.report" class="org.openmrs.module.kenyacore.report.ReportConfiguration">
		<property name="commonReports">
			<set>
				<ref bean="kenyaemr.etl.common.report.patientClinicalVisits"/>
				<ref bean="kenyaemr.etl.common.report.patientOnART"/>
			</set>
		</property>
	</bean>

	<bean id="kenyaemr.etl.common.report.patientClinicalVisits" class="org.openmrs.module.kenyacore.report.IndicatorReportDescriptor">
		<property name="targetUuid" value="598dce12-6714-4ba3-8f70-6050ae45e09e" />
		<property name="name" value="Patient Clinical Visits Report" />
		<property name="description" value="A report for Patient Clinical Visits" />
		<property name="apps">
			<set>
				<ref bean="kenyaemr.app.reports" />
			</set></property>
	</bean>

	<bean id="kenyaemr.etl.common.report.patientOnART" class="org.openmrs.module.kenyacore.report.HybridReportDescriptor">
		<property name="targetUuid" value="5ef336f5b-de69-47a2-9d55-71a88a96fb14" />
		<property name="name" value="Patients On ART Register" />
		<property name="description" value="A report for Patient On ART" />
		<property name="apps">
			<set>
				<ref bean="kenyaemr.app.reports" />
			</set></property>
	</bean>

</beans>
